<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true"/>
        <service id="shopware_bundle_mail.autocomplete_resolver.short_foreach_resolver" class="Shopware\Bundle\MailBundle\AutocompleteResolver\ShortForeachResolver">
            <tag name="shopware.mail.completer"/>
        </service>

        <service id="shopware_bundle_mail.autocomplete_resolver.foreach_resolver" class="Shopware\Bundle\MailBundle\AutocompleteResolver\ForeachResolver">
            <tag name="shopware.mail.completer"/>
        </service>

        <service id="shopware_bundle_mail.auto_complete_resolver" class="Shopware\Bundle\MailBundle\AutoCompleteResolver">
            <argument type="tagged" tag="shopware.mail.completer"/>
        </service>
    </services>
</container>